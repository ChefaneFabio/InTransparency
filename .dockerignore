*.test.ts
*.test.tsx
*.test.js
*.spec.ts
*.spec.tsx
*.spec.js
__tests__/
tests/
coverage/
.nyc_output/

# Temporary files
tmp/
temp/
*.tmp

# Backup files
*.backup
*.bak
*.old
node_modules.backup/

# JSON reports
*-report.json
*-audit.json
PAGE_AUDIT_REPORT.json

